<h1>Listing moves</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Match</th>
      <th>Square</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moves.each do |move| %>
      <tr>
        <td><%= move.user_id %></td>
        <td><%= move.match_id %></td>
        <td><%= move.square_id %></td>
        <td><%= move.value %></td>
        <td><%= link_to 'Show', move %></td>
        <td><%= link_to 'Edit', edit_move_path(move) %></td>
        <td><%= link_to 'Destroy', move, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Move', new_move_path %>
